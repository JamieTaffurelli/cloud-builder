        "settings": {
            "value": {
                "AntimalwareEnabled": "true",
                "RealtimeProtectionEnabled": "true",
                "ScheduledScanSettings": {
                    "isEnabled": "true",
                    "scanType": "Quick",
                    "day": "7",
                    "time": "120"
                }
            }
        },
        "tags": {
            "value": {
                "testdeploy": "testdeploy"
            }
        }
    }
}